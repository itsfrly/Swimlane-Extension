{
    "manifest_version": 2,
    "version":"version 2",
    "name":"Swimlane Fix",
    "description":"Javascript extension to fix scrollbar issues on Swimlane.io deployments.",
    "homepage_url":"https://github.com/itsfrly/SwimlaneFix-FireFox-Extension.git",

    "permissions": [
        "webNavigation"
    ],

    "content_scripts": [
        {
            "matches": ["https://megaplanit.pov.swimlane.io/*"],
            "js": ["fixCSS.js"]
        }
    ]
}   



